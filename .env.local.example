# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@clustername.mongodb.net/portfolio?retryWrites=true&w=majority

# NextAuth.js Secret
# Generate a secure secret: openssl rand -base64 32
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Admin Setup Key (for creating additional admin accounts)
ADMIN_SETUP_KEY=your_admin_setup_key_here 

# Redis Configuration
REDIS_URL=redis://default:admin@localhost:6379
REDIS_PREFIX=portfolio
